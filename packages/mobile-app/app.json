{
  "expo": {
    "name": "Learn To Win",
    "slug": "learn-to-win",
    "scheme": "learntowin",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#14142C"
