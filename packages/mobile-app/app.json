        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#14142C"
      },
      "package": "com.learntowin.learntowin",
      "versionCode": 1
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "0f3450ef-0b8d-435c-b08a-e7ce9cb890f2"
      }
    },
    "owner": "learn-to-win"
  },
  "plugins": [
    "@heap/react-native-heap"
  ]
